 <!-- Contenido del sitio -->
    <div class="container-fluid">
      <div class="row">
        <div class="center col-xs-12">  
          <h1 class="titulo1 t1">Acerca de nosotros</h1>
        </div>
      </div>
       <!-- Primera Parte -->
        <div class="row">
           
          <div class="page-header">
            <% @abouts.each do |about| %>

        			  <h1><%= about.title %></h1>
                <h3><small><%= about.desc %></small></h3>
        			  
            <% end %>
		    	</div>

        </div>
        <!-- /.row -->
       
        <!-- Segunda Parte -->
      	
        <div class="row">
          
	        <div class="col-xs-12 col-md-3 center">
            <%= image_tag ("Colaboracion.png"), class: "img-responsive3"  %>
	        </div>

          <div class="col-xs-12 col-md-3 center">
            <%= image_tag ("Pasion.png"), class: "img-responsive3"  %>
          </div>

          <div class="col-xs-12 col-md-3 center">
            <%= image_tag ("Liderazgo.png"), class: "img-responsive3"  %>
          </div>

          <div class="col-xs-12 col-md-3 center">
            <%= image_tag ("Integridad.png"), class: "img-responsive3"  %>
          </div>
      </div>

        <!-- Fin segunda parte -->
         <hr>
        <!-- Parte de analistas -->
      
         
       
       <% @missionvisions.each do |missionvision| %>
           <!-- Primera Parte -->
            <div class="row">
              <div class="container-fluid">
                <div class="col-xs-12 col-md-8">
                <h1 class="titulo1 t1">Visión</h1>
              </div>
                <div class="col-xs-12 col-md-9">
                  <p><%= missionvision.descvision %></p>
                </div>
                <div class="col-xs-12 col-md-3">
                  <%= image_tag ("Vision.png"), class: "img-responsive2 " %>
                </div>
               
              </div>
            </div>
            <!-- /.row -->
            
            <hr>
           
            <!-- Segunda Parte -->
            <div class="row">

              <div class="container-fluid">
                
                <h1 class="titulo1 t1 derecha">Misión</h1>
                <div class="col-xs-12 col-md-3">
                  <%= image_tag ("Mision.png"), class: "img-responsive2" %>
                </div>
                <div class="col-xs-12 col-md-9">
                  
                  <p><%= missionvision.descmission %></p>
                </div>
               
              </div>
             
          </div>
            <!-- Fin segunda parte -->
       <% end %> 
    


        <hr>

        <div class="row">
	        
 			

      <div class="container-fluid">
      	<div class="row">
            <div class="col-md-6 col-md-offset-3">
              <div class="well well-sm">
                <form class="form-horizontal" action="" method="post">
                <fieldset>
                  <legend class="text-center">Contactanos</legend>
          
                  <!-- Name input-->
                  <div class="form-group">
                    <label class="col-md-3 control-label" for="name">Nombre</label>
                    <div class="col-md-9">
                      <input id="name" name="name" type="text" placeholder="En Construcción..." class="form-control">
                    </div>
                  </div>
          
                  <!-- Email input-->
                  <div class="form-group">
                    <label class="col-md-3 control-label" for="email">Tu E-mail</label>
                    <div class="col-md-9">
                      <input id="email" name="email" type="text" placeholder="En Construcción..." class="form-control">
                    </div>
                  </div>
          
                  <!-- Message body -->
                  <div class="form-group">
                    <label class="col-md-3 control-label" for="message">Tu Mensaje</label>
                    <div class="col-md-9">
                      <textarea class="form-control" id="message" name="message" placeholder="En Construcción..." rows="5"></textarea>
                    </div>
                  </div>
          
                  <!-- Form actions -->
                  <div class="form-group">
                    <div class="col-xs-12  col-md-12 text-right">
                      <%= link_to "Enviar", pages_about_path, type: 'button', class: 'btn btn-primary' %>
                    </div>
                  </div>
                </fieldset>
                </form>
              </div>
            </div>
      	</div>
      </div>


        </div>


       

        

    </div>